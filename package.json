{
  "name": "@favid-inc/app-artist",
  "version": "2.0.1",
  "private": true,
  "author": "Favid Team",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --clear",
    "start:dev": "npm run env:set -- dev && npm start",
    "start:prod": "npm run env:set -- prod && npm start",
    "env:set": "./scripts/environment/set-env.sh",
    "build": "tsc -p ./tsconfig.json",
    "clean": "rimraf ./dist",
    "lint": "tslint -p ./tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "ci:install": "npm ci",
    "ci:lint": "npm run lint",
    "ci:build": "npm run env:set -- prod && npm run build",
    "conventional-changelog": "conventional-changelog",
    "version:changelog": "npm run conventional-changelog -- -i ./CHANGELOG.md -s"
  },
  "dependencies": {
    "@eva-design/eva": "^1.0.1",
    "@favid-inc/api": "^3.5.20",
    "axios": "^0.19.0",
    "expo": "^33.0.6",
    "expo-analytics": "^1.0.8",
    "expo-app-auth": "^5.0.1",
    "expo-av": "^5.0.2",
    "expo-camera": "^5.0.1",
    "expo-constants": "^5.0.1",
    "expo-file-system": "^5.0.1",
    "expo-media-library": "^5.0.1",
    "expo-permissions": "^5.0.1",
    "firebase": "^6.3.0",
    "react": "^16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-modal": "^11.3.1",
    "react-native-multiple-select": "^0.5.3",
    "react-native-swiper": "^1.5.14",
    "react-native-ui-kitten": "^4.0.5",
    "react-navigation": "^3.11.0",
    "react-number-format": "^4.0.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/runtime": "^7.1.2",
    "@types/axios": "^0.14.0",
    "@types/react": "^16.8.19",
    "@types/react-native": "^0.57.60",
    "@types/react-navigation": "^3.0.7",
    "@types/react-redux": "^7.1.1",
    "@types/redux": "^3.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "conventional-changelog-cli": "^2.0.21",
    "husky": "^1.1.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.1"
  }
}
